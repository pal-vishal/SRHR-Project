<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.social.actionindia">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_srhr_final_logo_eps"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.social.actionindia.model.LanguageActivity"></activity>

        <activity android:name="com.social.actionindia.Clinics" />
        <activity android:name="com.social.actionindia.pwdvaact" />
        <activity android:name="com.social.actionindia.Email" />
        <activity android:name="com.social.actionindia.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity> <!-- <activity android:name=".safesex" /> -->
        <activity android:name="com.social.actionindia.LastScreenMenu.TermsConditions" />
        <activity android:name="com.social.actionindia.Videos" />
        <activity android:name="com.social.actionindia.MainActivity" />
    </application>

</manifest>